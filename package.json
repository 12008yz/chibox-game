{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "workers": "node ./scripts/start-workers.js",
    "dev": "concurrently \"npm start\" \"npm run workers\"",
    "queue:stats": "node -e \"require('./services/queueService').getQueueStats().then(console.log)\"",
    "queue:clean": "node -e \"require('./services/queueService').cleanQueues().then(() => console.log('Queues cleaned'))\"",
    "security-audit": "npm audit",
    "security-fix": "npm audit fix",
    "security-lint": "eslint . --ext .js",
    "security-full": "npm run security-audit && npm run security-snyk && npm run security-lint",
    "system:start": "node ./scripts/start-system.js",
    "system:stop": "pm2 stop all",
    "system:restart": "pm2 restart all",
    "system:status": "pm2 status",
    "system:logs": "pm2 logs",
    "system:monit": "pm2 monit",
    "cron:start": "node ./scripts/setup-cron-improved.js",
    "cron:legacy": "node ./scripts/setup-cron.js"
  },
  "dependencies": {
    "@fastify/csrf-protection": "^6.0.0",
    "@playwright/test": "^1.52.0",
    "argon2": "^0.43.0",
    "axios": "^1.9.0",
    "bull": "^4.16.5",
    "cheerio": "^1.0.0",
    "compression": "^1.8.0",
    "connect-session-sequelize": "^7.1.7",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "ngrok": "^5.0.0-beta.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-steam": "^1.0.18",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "playwright-core": "^1.52.0",
    "pug": "^3.0.3",
    "puppeteer": "^24.10.0",
    "redis": "^5.1.1",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "steam-community": "^0.2.2",
    "steam-totp": "^2.1.2",
    "steam-tradeoffer-manager": "^2.11.7",
    "steam-user": "^5.2.1",
    "steamcommunity": "^3.48.6",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.0",
    "concurrently": "^7.6.0",
    "eslint": "^9.27.0",
    "eslint-plugin-security": "^3.0.1",
    "jest-environment-node": "^30.0.0",
    "selfsigned": "^2.4.1",
    "sequelize-cli": "^6.6.2"
  }
}
